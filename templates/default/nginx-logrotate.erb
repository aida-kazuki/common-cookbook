# Generated by Chef for <%= node['hostname'] %>.
# Local modifications will be overwritten.

<%= @nginx_logs.join(" ") %> {
    daily
    missingok
    rotate 30
    dateext
    compress
    delaycompress
    notifempty
    sharedscripts
    postrotate
        [ -f /var/run/nginx.pid ] && kill -USR1 `cat /var/run/nginx.pid`
    endscript
}

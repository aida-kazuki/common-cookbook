# Generated by Chef for <%= node['hostname'] %>.
# Local modifications will be overwritten.
<%- node.sort.each do |k, v| %>
  <%- if v.instance_of?(Array) %>
<%= k %>:
    <%- v.each do |vv| %>
    <%= vv %>
    <%- end %>
  <%- elsif v.instance_of?(Hash) %>
<%= k %>:
    <%- v.each do |kk, vv| %>
    <%= kk %>: <%= vv %>
    <%- end %>
  <%- elsif v.instance_of?(Mash) %>
<%= k %>:
    <%- v.each do |kk, vv| %>
      <%- if vv.instance_of?(Mash) %>
  <%= kk %>: <%= vv.inspect %>
      <%- else %>
  <%= kk %>: <%= vv %>
      <%- end %>
    <%- end %>
  <%- else %>
<%= k %>: <%= v %>
  <%- end %>
<%- end %>
